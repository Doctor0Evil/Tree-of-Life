SECTION,NATURE.FAIRNESS-CORRIDOR,V1
ROW,ROLE,DIAGNOSTIC-ONLY
ROW,NOACTUATION,TRUE
ROW,NOCAPABILITYWRITE,TRUE
ROW,NOENVELOPEWRITE,TRUE
ROW,LOGSTREAMS,.evolve.jsonl,.donutloop.aln

---- Inputs per FateWindow / episode ----
ROW,INPUTS,
  FATEWINDOW.ID,
  FATEWINDOW.VALID,              -- standard validity flag
  FATEWINDOW.ROH_MAX,            -- max RoH in window
  FATEWINDOW.ROH_CEILING,        -- should be 0.30 for governed humans
  FATEWINDOW.LIFEFORCE_MIN,      -- min lifeforce across governed roles
  FATEWINDOW.LIFEFORCE_FLOOR,    -- configured minsafe floor
  FATEWINDOW.UNFAIRDRAIN,        -- bool aggregate UNFAIRDRAIN flag
  JUSTICE.ERG,                   -- Environmental Resilience Grade
  JUSTICE.ERG_MIN_BAND,          -- corridor band for ERG
  JUSTICE.TECR,                  -- Tokenâ€‘Enforced Collapse Rate
  JUSTICE.TECR_MAX_BAND          -- maximum acceptable collapse band

---- Component predicates ----
-- RoH corridor: RoH never exceeds its ceiling and window is valid.
ROW,PREDICATE,NATURE.FAIRNESS.ROH_OK,
  FATEWINDOW.VALID == true &&
  FATEWINDOW.ROH_MAX <= FATEWINDOW.ROH_CEILING

-- Lifeforce floor: no governed role drops below lifeforce floor.
ROW,PREDICATE,NATURE.FAIRNESS.LIFEFORCE_OK,
  FATEWINDOW.LIFEFORCE_MIN >= FATEWINDOW.LIFEFORCE_FLOOR

-- Fairness: UNFAIRDRAIN must remain false for all protected roles.
ROW,PREDICATE,NATURE.FAIRNESS.UNFAIRDRAIN_OK,
  FATEWINDOW.UNFAIRDRAIN == false

-- Ecological resilience: ERG inside safe band.
ROW,PREDICATE,NATURE.FAIRNESS.ERG_OK,
  JUSTICE.ERG >= JUSTICE.ERG_MIN_BAND

-- Systemic stability: TECR below configured maximum band.
ROW,PREDICATE,NATURE.FAIRNESS.TECR_OK,
  JUSTICE.TECR <= JUSTICE.TECR_MAX_BAND

-- Corridor conjunction: NATURE.FAIRNESS holds only when all are true.
ROW,PREDICATE,NATURE.FAIRNESS,
  NATURE.FAIRNESS.ROH_OK &&
  NATURE.FAIRNESS.LIFEFORCE_OK &&
  NATURE.FAIRNESS.UNFAIRDRAIN_OK &&
  NATURE.FAIRNESS.ERG_OK &&
  NATURE.FAIRNESS.TECR_OK

---- Invariants (diagnostic-only, no control surface) ----
ROW,INVARIANT,DIAGNOSTIC-ONLY,
  ROLE == DIAGNOSTIC-ONLY &&
  NOACTUATION == TRUE &&
  NOCAPABILITYWRITE == TRUE &&
  NOENVELOPEWRITE == TRUE

-- Forbid direct use as guards in kernels / capabilities.
ROW,INVARIANT,NO-KERNEL-GUARD-NATURE-FAIRNESS,
  FORBID_SYMBOL_IN,KERNEL.GUARDS,NATURE.FAIRNESS

ROW,INVARIANT,NO-KERNEL-GUARD-UNFAIRDRAIN,
  FORBID_SYMBOL_IN,KERNEL.GUARDS,FATEWINDOW.UNFAIRDRAIN

ROW,INVARIANT,NO-KERNEL-GUARD-JUSTICE-METRICS,
  FORBID_SYMBOL_IN,KERNEL.GUARDS,JUSTICE.ERG,JUSTICE.TECR

---- Serialization contract ----
ROW,OUTPUTS,
  NATURE.FAIRNESS,
  NATURE.FAIRNESS.ROH_OK,
  NATURE.FAIRNESS.LIFEFORCE_OK,
  NATURE.FAIRNESS.UNFAIRDRAIN_OK,
  NATURE.FAIRNESS.ERG_OK,
  NATURE.FAIRNESS.TECR_OK

ROW,EVOLVE.JSONL.MAP,
  FIELD,nature_fairness,
  SOURCE,NATURE.FAIRNESS

ROW,EVOLVE.JSONL.MAP,
  FIELD,nature_fairness_components,
  SOURCE,
    NATURE.FAIRNESS.ROH_OK,
    NATURE.FAIRNESS.LIFEFORCE_OK,
    NATURE.FAIRNESS.UNFAIRDRAIN_OK,
    NATURE.FAIRNESS.ERG_OK,
    NATURE.FAIRNESS.TECR_OK

ROW,DONUTLOOP.ALN.MAP,
  SECTION,NATURE.FAIRNESS.SUMMARY,
  FIELDS,
    FATEWINDOW.ID,
    NATURE.FAIRNESS,
    NATURE.FAIRNESS.ROH_OK,
    NATURE.FAIRNESS.LIFEFORCE_OK,
    NATURE.FAIRNESS.UNFAIRDRAIN_OK,
    NATURE.FAIRNESS.ERG_OK,
    NATURE.FAIRNESS.TECR_OK
